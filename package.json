{
  "name": "solana-web3auth-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@web3auth/base": "^9.0.0",
    "@web3auth/modal": "^9.0.0",
    "@web3auth/solana-provider": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.0.0",
    "gill": "^0.12.0",
    "jose": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.0"
  },
  "create-solana-dapp": {
    "name": "Web3Auth Template",
    "description": "Next.js template with Web3Auth social login and embedded wallet generation for Solana",
    "category": "community",
    "framework": "nextjs",
    "features": [
      "Web3Auth integration",
      "Social login (Google, Twitter, Discord, GitHub)",
      "Email passwordless authentication",
      "SMS authentication",
      "Embedded wallet generation",
      "Session management",
      "Protected routes",
      "User profile management"
    ],
    "postInstall": [
      "npm install",
      "cp .env.example .env.local",
      "Configure your Web3Auth dashboard at https://dashboard.web3auth.io",
      "Update .env.local with your Web3Auth client ID and other configuration",
      "npm run dev"
    ]
  }
}
