{
  "name": "Web3Auth Template",
  "description": "Next.js template with Web3Auth social login and embedded wallet generation for Solana",
  "category": "community",
  "framework": "nextjs",
  "features": [
    "Web3Auth integration",
    "Social login (Google, Twitter, Discord, GitHub)",
    "Email passwordless authentication",
    "SMS authentication",
    "Embedded wallet generation",
    "Session management",
    "Protected routes",
    "User profile management",
    "Wallet operations (sign, send, balance)"
  ],
  "tags": [
    "web3auth",
    "authentication",
    "social-login",
    "wallet",
    "solana",
    "nextjs",
    "typescript",
    "tailwind"
  ],
  "author": "Solana Foundation",
  "version": "1.0.0",
  "repository": "https://github.com/solana-foundation/templates/tree/main/community/web3-auth",
  "documentation": "https://github.com/solana-foundation/templates/tree/main/community/web3-auth/README.md",
  "postInstall": [
    "npm install",
    "cp env.example .env.local",
    "echo 'Configure your Web3Auth dashboard at https://dashboard.web3auth.io'",
    "echo 'Update .env.local with your Web3Auth client ID and other configuration'",
    "npm run dev"
  ],
  "requirements": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@solana/web3.js": "^1.98.0",
    "@web3auth/modal": "^10.3.0",
    "@web3auth/base": "^10.3.0",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "jose": "^5.2.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.0"
  }
}
